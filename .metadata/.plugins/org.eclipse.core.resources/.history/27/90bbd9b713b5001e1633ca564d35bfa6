package Week_4_workshop;

public class question5 {

    public static void main(String[] args) {
        Bankaccount b = new Bankaccount();
        Bankaccount b1 = new Bankaccount("Samyog", 1234, 42);
    }
}

class Bankaccount {
    String Name;
    int accountnumber;
    int balance;

    Bankaccount() {
        System.out.println("The details are: ");
    }

    Bankaccount(String s, int n, int nu) {
        System.out.println("Name: " + s);
        System.out.println("Account number: " + n);
        System.out.println("Balance: " + nu);
    }
}