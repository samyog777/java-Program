package Tutorial_5;

public class animal {

	public static void main(String[] args) {
		DOg d = new DOg();
		d.eat();
		d.bark();
//		Anima a = new Anima();
//		a.eat();
//		Cat c = new Cat();
//		c.meow();
	}
}


class Anima {
	void eat() {
		System.out.println("Animal is eating!");
	}
}

class DOg extends Anima {
	void bark() {
		System.out.println("Dog is barking");
	}
}

class Cat extends Anima {
	void meow() {
		System.out.println("Cat is neowing");
	}
}
